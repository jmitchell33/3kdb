#alias gem-stat-lookup {
    #var foundGem 0;
    #format tempName {%l} {%0};

    #foreach $crafting[data][gems] gem {
        #format listName {%l} $gem[effect];
        #regex {$listName} {%*${tempName}%*} {
            #var foundGem 1;
            #echo {<ece>-----  $gem[gem] Information  -----};
            #echo {<fff>  Minimum Level: $gem[min-level]};
            #echo {<fff>  Maximum Level: $gem[min-level]};
            #echo {<fff>  Gem Effects: $gem[effect]};
            #echo {<fff>  Jewels Required:};
            #foreach $gem[jewels] jewel {
                #echo {<fff>           $jewel};
            };
            #echo {<fff>  Fragments Required: $gem[fragment]};
            #echo {<ece>-----------------------------------};
        };
    };
    #if !$foundGem {
        #echo {<edd>-----   UNABLE TO FIND A GEM MATCHING <fff> %0  <edd>   -----};
    };
};



