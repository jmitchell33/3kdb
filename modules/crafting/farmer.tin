        #var craft_farming[data][goblin_red] {$tmpValue}
    };
    #elseif {"$tmpColor" == "blue"} {
        #var craft_farming[data][goblin_blue] {$tmpValue}
    };
    #elseif {"$tmpColor" == "green"} {
