#nop plant detection - basic (no owner);
#action {^-i-A sproutling %* plant.$} {
    #var craft_farming[data][room_has_plant] 1;
    
    #nop Capture size and set growth percentage;
    #var craft_farming[data][plant_size] sproutling;
    
    #if {&craft_farming[sizes][%1]} {
        #var craft_farming[data][ideal_growth] {$craft_farming[sizes][%1]}
    };
    #else {
        #var craft_farming[data][ideal_growth] 5
    };
    
    #if {$craft_farming[data][echo] == 1} {
        #echo {<afa>Found plant: %1 %2 (Growth: $craft_farming[data][ideal_growth]%%)<088>}
    }
} {2};

#nop plant detection - with owner (captures size and updates growth);
#action {^-i-A {sproutling|tiny|puny|little|knee-height|chest-height|person-sized|towering|massive|GIGANTIC|harvestable} %* plant, owned by %*.$} {
    #var craft_farming[data][room_has_plant] 1;
    #var craft_farming[data][plant_owner] {%3};
    
    #nop Capture size and set growth percentage;
    #var craft_farming[data][plant_size] {%1};
    
    #if {&craft_farming[sizes][%1]} {
        #var craft_farming[data][ideal_growth] {$craft_farming[sizes][%1]}
    };
    #else {
        #var craft_farming[data][ideal_growth] 5
    };
    
    #if {$craft_farming[data][echo] == 1} {
        #echo {<afa>Found plant: %1 %2 owned by %3 (Growth: $craft_farming[data][ideal_growth]%%)<088>}
    };
    
    #if {$craft_farming[data][farm_status] == 1} {
        show_farm_status
    }
} {2};

