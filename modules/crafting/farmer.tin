#nop ============================================;
#nop MESSAGE HANDLING ALIASES;
#nop ============================================;

#alias do_grow_message {
    #if {$craft_farming[data][echo] == 1} {
        #echo {<ffa>Round results: Growth <fff>(msg %1)<088>}
    };
    #math craft_farming[data][plantround] {$craft_farming[data][plantround] + 1};
    
    #if {$craft_farming[data][farm_status] == 1} {
        show_farm_status
    };
    
    plantaction
};

#alias do_neutral_message {
    #if {$craft_farming[data][echo] == 1} {
        #echo {<feb>Round results: NO Growth <fff>(msg %1)<088>}
    };
    #math craft_farming[data][plantround] {$craft_farming[data][plantround] + 1};
    
    #if {$craft_farming[data][farm_status] == 1} {
        show_farm_status
    };
    
    plantaction
};

#alias do_shrink_message {
    #if {$craft_farming[data][echo] == 1} {
        #echo {<faa>Round results: Shrinking <fff>(msg %1)<088>}
    };
    #math craft_farming[data][plantround] {$craft_farming[data][plantround] + 1};
    
    #if {$craft_farming[data][farm_status] == 1} {
        show_farm_status
    };
    
    plantaction
};

#alias do_death_message {
    stop_farming;
    
    #if {$craft_farming[random][active] == 1} {
        #echo {<faa>Random farming failed - plant died.<088>};
        #var craft_farming[random][active] 0;
        #var craft_farming[random][log] {};
        #list craft_farming[random][actions] clear
    };
    
    reset_farm_variables;
    
    #if {$craft_farming[data][echo] == 1} {
        #echo {<faa>===========================================<088>};
        #echo {<faa>Round results: Dead <fff>(msg %1)<088>};
        #echo {<fff>Farming stopped and variables reset<088>};
        #echo {<faa>===========================================<088>}
    };
    
    #if {$craft_farming[data][auto_active] == 1} {
        continue_auto_farming
    }
};

