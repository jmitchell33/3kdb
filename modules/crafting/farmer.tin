#nop harvest inventory tracking;
#action {^The %w %* tumbles to your feet.$} {
    #local quality {%1};
    #local crop_name {%2};
    #local full_name {$quality $crop_name};
    
    #nop Increment harvest count;
    #math craft_farming[data][harvest_count] {$craft_farming[data][harvest_count] + 1};
    
    #nop Track in harvest inventory;
    #if {&craft_farming[harvests][$full_name]} {
        #math craft_farming[harvests][$full_name] {$craft_farming[harvests][$full_name] + 1}
    };
    #else {
        #var craft_farming[harvests][$full_name] 1
    };
    
    #if {$craft_farming[data][echo] == 1} {
        #echo {<ffa>Harvested:<fff> $full_name (Total this session: $craft_farming[data][harvest_count])<088>}
    }
} {2};

