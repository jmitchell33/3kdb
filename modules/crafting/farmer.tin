#action {^  Farming    : %s%d / %s%d \(%s%d\%\) \[%s%*%s\]$} {
    #var craft_farming[data][farming_level] %2;
    setfarmlevel $craft_farming[data][farming_level];
    #if {$craft_farming[data][echo] == 1} {
        #echo {<ffa>Farming level automatically detected:<088> %2}
    }
} {2};

