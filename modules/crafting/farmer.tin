#nop ============================================;
#nop AUTO FARMING SYSTEM;
#nop ============================================;

#nop calculate valid farm lands;
#alias calculate_valid_farms {
    #list craft_farming[farm_list] clear;
    
    #loop {1} {15} {tmpfarm} {
        #local min_lvl {$craft_farming[farmland][min][$tmpfarm]};
        #local max_lvl {$craft_farming[farmland][max][$tmpfarm]};
        
        #if {"$craft_farming[data][auto_mode]" == "all"} {
            #if {$craft_farming[data][farming_level] >= $min_lvl && $craft_farming[data][farming_level] <= $max_lvl} {
                #list craft_farming[farm_list] add {$tmpfarm};
                #if {$craft_farming[data][echo] == 1} {
                    #echo {<caf>Valid farm: farm_land$tmpfarm ($min_lvl-$max_lvl)<088>}
                }
            }
        };
        #elseif {"$craft_farming[data][auto_mode]" == "cxp"} {
            #if {$craft_farming[data][farming_level] >= $min_lvl  && $craft_farming[data][farming_level] <= $max_lvl} {
                #list craft_farming[farm_list] add {$tmpfarm};
                #if {$craft_farming[data][echo] == 1} {
                    #echo {<caf>Valid CXP farm: farm_land$tmpfarm ($min_lvl-$max_lvl)<088>}
                };
            };
        };
    };
    
    #list craft_farming[farm_list] size craft_farming[data][max_farms];
    
    #if {$craft_farming[data][echo] == 1} {
        #echo {<caf>Found $craft_farming[data][max_farms] valid farm(s)<088>}
    }
};

#alias start_auto_farming {
    #if {$craft_farming[data][auto_active] == 1} {
        #echo {<faa>Auto-farming already active!<088>};
        #return
    };
    
    #if {$craft_farming[data][farming_level] == 0} {
        #echo {<faa>Please set farming level first: <fff>setfarmlevel <level><088>};
        #return
    };
    
    #var craft_farming[data][auto_active] 1;
    #var craft_farming[data][current_farm_num] 0;
    
    calculate_valid_farms;
    
    #if {$craft_farming[data][max_farms] == 0} {
        #echo {<faa>No valid farms found for your level!<088>};
        #var craft_farming[data][auto_active] 0;
        #return
    };
    
    #echo {<caf>Found $craft_farming[data][max_farms] valid farm(s)!<088>};
    #echo {<ffa> STEP 1: GOING TO FARM STORE <088>};
    
    #if {$craft_farming[data][travel_toggle] == 1} {
        #if {$craft_farming[data][go_toggle] == 0} {
            #var craft_farming[data][go_toggle] 1;
            go farm_store;
            #delay {10} {
                #var craft_farming[data][go_toggle] 0;
                #echo {<ffa> STEP 2: PURCHASING SUPPLIES <088>};
                auto_buy_supplies
            }
        }
    };
    #else {
        gofarmstore;
        #delay {5} {
            #echo {<ffa> STEP 2: PURCHASING SUPPLIES <088>};
            auto_buy_supplies
        }
    }
};

#alias auto_buy_supplies {
    #echo {<ffa> Purchasing supplies...<088>};
    buysupplies all;
    
    #delay {5} {
        #echo {<fff> <ffa>Supplies purchased!<088>};
        #echo {<ffa> STEP 3: HEADING TO FIRST FARM <088>};
        
        #if {$craft_farming[data][travel_toggle] == 0} {
            homefarmstore;
            #delay {3} {farm_next_location}
        };
        #else {
            farm_next_location
        }
    }
};

#alias farm_next_location {
    #math craft_farming[data][current_farm_num] {$craft_farming[data][current_farm_num] + 1};
    
    #if {$craft_farming[data][current_farm_num] > $craft_farming[data][max_farms]} {
        #echo {<caf> ALL FARMS COMPLETE! <088>};
        stop_auto_farming;
        #return
    };
    
    #local next_farm {$craft_farming[farm_list][$craft_farming[data][current_farm_num]]};
    #var craft_farming[data][current_farm_id] {$next_farm};
    #local farm_room {farm_land$next_farm};
    
    #echo {<caf> FARM $craft_farming[data][current_farm_num] of $craft_farming[data][max_farms] (farm_land$next_farm) <088>};
    
    #if {$craft_farming[data][travel_toggle] == 1} {
        #if {$craft_farming[data][go_toggle] == 0} {
            #var craft_farming[data][go_toggle] 1;
            go $farm_room;
            #delay {10} {
                #var craft_farming[data][go_toggle] 0;
                #path destroy;
                #delay {2} {
                    #if {$craft_farming[data][echo] == 1} {
                        #echo {<caf>Arrived at farm_land$next_farm<088>}
                    };
                    farm_start_current
                }
            }
        }
    };
    #else {
        gofarm$next_farm;
        #delay {7} {
            #if {$craft_farming[data][echo] == 1} {
                #echo {<caf>Arrived at farm_land$next_farm<088>}
            };
            farm_start_current
        }
    }
};

#alias farm_start_current {
    #var craft_farming[data][path_index] 0;
    #list craft_farming[path_directions] clear;
    #list craft_farming[path_directions] create {e} {e} {e} {n} {w} {w} {n} {e} {e} {n} {w} {w} {w} {s} {s} {s};
    
    #if {$craft_farming[data][echo] == 1} {
        #echo {<afa>Starting first room - checking plant...<088>}
    };
    
    farm_check_plant
};

#alias farm_check_plant {
    #var craft_farming[data][room_has_plant] 0;
    #var craft_farming[data][plant_owner] {};
    
    #if {$craft_farming[data][echo] == 1} {
        #echo {<afa>Checking for plant...<088>}
    };
    
    sne look;
    
    #delay {2} {
        #if {$craft_farming[data][room_has_plant] == 0} {
            #if {$craft_farming[data][echo] == 1} {
                #echo {<feb>No plant - moving on<088>}
            };
            #if {$craft_farming[data][auto_active] == 1} {
                farm_walk_next_room
            }
        };
        #else {
            #if {"$craft_farming[data][plant_owner]" != ""} {
                #echo {<faa>Plant owned by $craft_farming[data][plant_owner] - skipping<088>};
                farm_walk_next_room
            };
            #else {
                sne look at plant
            }
        }
    }
};

#alias farm_walk_next_room {
    #math craft_farming[data][path_index] {$craft_farming[data][path_index] + 1};
    
    #local path_total {&craft_farming[path_directions][]};
    
    #if {$craft_farming[data][path_index] > $path_total} {
        #if {$craft_farming[data][echo] == 1} {
            #echo {<caf>All rooms in farm_land$craft_farming[data][current_farm_id] complete!<088>}
        };
        #if {$craft_farming[data][travel_toggle] == 0} {
            homefarm$craft_farming[data][current_farm_id];
            #delay {7} {farm_next_location}
        };
        #else {
            sne wash hands;
            #delay {3} {farm_next_location}
        };
        #return
    };
    
    #local next_dir {$craft_farming[path_directions][$craft_farming[data][path_index]]};
    
    #if {$craft_farming[data][echo] == 1} {
        #echo {<feb>Moving: $next_dir (room $craft_farming[data][path_index] of $path_total)<088>}
    };
    
    sne $next_dir;
    
    #delay {2} {farm_check_plant}
};

#alias continue_auto_farming {
    #if {$craft_farming[data][auto_active] == 1} {
        #delay {2} {farm_walk_next_room}
    }
};

#alias stop_auto_farming {
    #var craft_farming[data][auto_active] 0;
    #var craft_farming[data][auto_mode] {manual};
    #var craft_farming[data][current_farm_num] 0;
    #var craft_farming[data][path_index] 0;
    
    #if {$craft_farming[data][travel_toggle] == 1} {
        #if {$craft_farming[data][go_toggle] == 0} {
            #var craft_farming[data][go_toggle] 1;
            go farming;
            #delay {7} {
                #var craft_farming[data][go_toggle] 0;
                #echo {<caf>Auto-farming stopped - Returned to farming area<088>}
            }
        }
    };
    #else {
        #echo {<caf>Auto-farming stopped<088>}
    }
};

#alias fr {
    #switch {"%1"} {
        #case {""} {
            #echo {<ffa>Usage: <fff>fr <all|cxp><088>};
            #echo {<feb>  <ccd>fr all<feb> - Farm all available farms<088>};
            #echo {<feb>  <ccd>fr cxp<feb> - Farm CXP farms only<088>}
        };
        #case {"all"} {
            #var craft_farming[data][auto_mode] all;
            start_auto_farming
        };
        #case {"cxp"} {
            #var craft_farming[data][auto_mode] cxp;
            start_auto_farming
        };
        #default {
            #echo {<faa>Invalid option: <fff>%1<088>};
            #echo {<feb>Use: <ccd>fr all<feb> or <ccd>fr cxp<088>}
        }
    }
};

#alias farm {
    #switch {"%1"} {
        #case {""} {
            #echo {<faa><088>};
            #echo {<ffa>FARM COMMAND USAGE:<088>};
            #echo {<faa><088>};
            #echo {  <ffc>farm all<feb>     - Farm all available farms based on your skill<088>};
            #echo {  <ffc>farm cxp<feb>     - Farm CXP-giving farms only (advanced)<088>};
            #echo {  <ffc>fr all<feb>       - Quick start: farm all<088>};
            #echo {  <ffc>fr cxp<feb>       - Quick start: farm CXP<088>};
            #echo {<faa><088>};
            #echo {<feb>Type <ccd>farmhelp autofarming<feb> for detailed help<088>};
            #echo {<faa><088>}
        };
        #case {"all"} {
            #if {$craft_farming[data][farming_level] == 0} {
                #echo {<ffa>Please set your farming level first: <ccd>setfarmlevel <level><088>}
            };
            #else {
                #var craft_farming[data][auto_mode] all;
                #echo {<ffa>Starting AUTO FARM ALL mode - Farming until complete!<088>};
                start_auto_farming
            }
        };
        #case {"cxp"} {
            #if {$craft_farming[data][farming_level] == 0} {
                #echo {<ffa>Please set your farming level first: <ffc>setfarmlevel <level><088>}
            };
            #else {
                #var craft_farming[data][auto_mode] cxp;
                #echo {<ffa>Starting FARM CXP mode - CXP farms only!<088>};
                start_auto_farming
            }
        };
        #default {
            #echo {<faa>Invalid option: <fff>%1<088>};
            #echo {<feb>Type <ccd>farm<feb> for usage information<088>}
        }
    }
};

#alias setfarmlevel {
    #if {"%1" == ""} {
        #echo {<faa>Current farming level: <fff>$craft_farming[data][farming_level]<088>};
        #echo {<feb>Usage: <ccd>setfarmlevel <level><088>}
    };
    #else {
        #var craft_farming[data][farming_level] {%1};
        #echo {<ffa>Farming level set to: <fff>%1<088>};
        
        #nop Calculate available farms;
        #local farm_count 0;
        #loop {1} {15} {tmpfarm} {
            #if {%1 >= $craft_farming[farmland][min][$tmpfarm] && %1 <= $craft_farming[farmland][max][$tmpfarm]} {
                #math farm_count {$farm_count + 1}
            }
        };
        
        #echo {<feb>Available farms: <fff>$farm_count<feb> farms unlocked<088>}
    }
};

#alias stopauto {
    #if {$craft_farming[data][auto_active] == 1} {
        #echo {<faa>Stopping auto-farming...<088>};
        stop_auto_farming
    };
    #else {
        #echo {<feb>Auto-farming is not currently active<088>}
    }
};

