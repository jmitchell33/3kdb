#alias farmgoods {
    #local border {====================================};
    #echo {<faa>$border<088>};
    #echo {<ffa>    Harvested Farm Goods<088>};
    #echo {<faa>$border<088>};
    
    #if {&craft_farming[harvests][] == 0} {
        #echo {<feb>  No harvests recorded this session<088>}
    };
    #else {
        #foreach {*craft_farming[harvests][%*]} {crop} {
            #format crop_display {<ccd>  %-3s <feb>%s} {$craft_farming[harvests][$crop]} {$crop};
