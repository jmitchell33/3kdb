#NOP -- function to also take a step back on the mapper if we undo a map;
#alias mapUndo {
	#map undo;
	#if {$bot[active] && !$bot[paused]} {
		#math bot[step] {$bot[step] - 1};
		#if {!$incombat} {
			#delay .5 {look};
		};
	};
};


