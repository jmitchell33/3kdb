#NOP -- aliases for setting up summon config & strategies;
#alias {killmode} {
    #switch {"%0"} {
        #NOP -- view available options;
        #case {"help"} {
            #echo {<fff>Available killmodes:<088>};
            #echo {<eaf>No argument <bec>(2 summons defending & 2 summons assisting)<088>};
            #echo {<eaf>2 <bec>(4 summons assisting)<088>};
            #echo {<eaf>3 <bec>(4 summons defending)<088>};
            #echo {<eaf>4 <bec>(3 summons assisting and 1 summon defending)<088>};
            #echo {<eaf>5 <bec>(1 summon defending and 3 summons assisting)<088>};
            #echo {<eaf>golem <bec>(golem assisting)<088>};
        };
        #NOP -- 4 summons assisting;
        #case {"2"} {
            clear_sconfig;
            shoo;
            shoo golem;
            mystrategy;
            ms 9##4#1500#70#assist#summons#always;
            ms golema;
        };
        #NOP -- 4 summons defending;
        #case {"3"} {
            clear_sconfig;
            shoo;
            shoo golem;
            mystrategy;
            ms 9##4#1500#70#defend#summons#always;
            ms golema;
        };
        #NOP -- 3 summons assisting and 1 summon defending;
        #case {"4"} {
            clear_sconfig;
            shoo;
            shoo golem;
            mystrategy;
            ms 9##3#1500#70#assist#summons#always;
            ms 9##1#1500#70#defend#summons#always;
            ms golema;
        };
        #NOP -- 1 summon defending and 3 summons assisting;
        #case {"5"} {
            clear_sconfig;
            shoo;
            shoo golem;
            mystrategy-golem;
            ms 9##1#1500#70#defend#summons#always;
            ms 9##3#1500#70#assist#summons#always;
            ms golemd;
        };
        #NOP -- offensive mage - golem assisting;
        #case {"golem"} {
            #echo {<ffa>No argument provided - running default killmode<088>};
            clear_sconfig;
            shoo;
            shoo golem;
            mystrategy;
            ms golema;
        };
        #NOP -- default for summoner 2 summons defending and 2 summons assisting;
        #case {""} {
            #echo {<ffa>No argument provided - running default killmode<088>};
            clear_sconfig;
            shoo;
            shoo golem;
            mystrategy;
            ms 9##2#1500#70#defend#summons#always;
            ms 9##2#1500#70#assist#summons#always;
            ms golema;
        };
        #default {
            #echo {<fff>Unknown killmode '%0'. Use 'killmode help' to see available modes.<088>};
        };
    };
};

