#NOP -- set variable for how many bags to cycle thru;
#alias {bagset} {
    #if {"%0" != ""} {
        #var bagcount %0;
        #echo {<ffa>BAG COUNT SET TO %0<088>};
    } {
        #echo {<faa>NO USER INPUT!<088>};
    };
};

#NOP -- cycle bags;
#alias {bag1} {
    #var bagsFull 1;
    #format tmpBagCount {%d} $bagcount;
    #loop $tmpBagCount 1 tmpBag {
        put all in bag $tmpBag;
    };
    #line oneshot #action {You put %* in %*.} {#var bagsFull 0};
};

#alias {bag2} {
    #format tmpBagCount {%d} $bagcount;
    drop all;
    #loop 1 $tmpBagCount tmpBag {
        get all from bag $tmpBag;drop all;
    };
    #var bagsFull 0;
};

