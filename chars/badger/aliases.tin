	#echo {<ccf>  -------<088>           <ffa>3kdb<088>           <ccf>-------<088>  };
    #echo {<ccf>  -------    <ffa>BADGERS ALIASES<088> <fff>($number)<088>  <ccf>-------    <088>  };
