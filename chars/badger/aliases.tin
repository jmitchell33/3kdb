    } {
        #echo {<faa>NO USER INPUT!}
    };
};

#NOP -- cycle bags;
#alias {bag1} {
    #var bagsFull 1;
    #format tmpBagCount {%d} $bagcount;
    #loop $tmpBagCount 1 tmpBag {
        put all in bag $tmpBag
    };
    #line oneshot #action {You put %* in %*.} {#var bagsFull 0};
};

#alias {bag2} {
    #format tmpBagCount {%d} $bagcount;
    drop all;
    #loop 1 $tmpBagCount tmpBag {
        get all from bag $tmpBag;drop all
    };
    #var bagsFull 0;
};

