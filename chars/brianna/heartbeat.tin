#class {player_heartbeat} {kill}
#class {player_heartbeat} {open}

#var corpse_delay 1;
#var corpse_use 200;
#var dismiss_support 1;

#alias {_player_heartbeat} {
    #if !$idle_flag {
        #if {$bot[active] == 1 && "$shp" != "" && $dismiss_support && $my[clan] && $shp < 25} {su1};
    };
};

#class {player_heartbeat} {close}