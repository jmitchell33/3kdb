#NOP -- deadman;
#alias {_player_deadmanActions} {
     #if {$bot[active]} {
          .pause;
          #echo {stepper paused due to deadman timer}
     };
};

#NOP --NUMPAD ON OR OFF
#alias {numoff} {#class numpad kill}
